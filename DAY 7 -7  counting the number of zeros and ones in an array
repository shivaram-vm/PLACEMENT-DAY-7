#include <stdio.h>

int main() {
    int n;
    
    scanf("%d", &n);
    
    int arr[n];
    int zero_count = 0, one_count = 0;


    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] == 0) {
            zero_count++;
        } else if (arr[i] == 1) {
            one_count++;
        }
    }
   
    printf("zc = %d\n", zero_count);
    printf("oc = %d\n", one_count);
    
    return 0;
}
